<!--Checks to see if a property is Land or not.  If it is land, then this changes the information
displayed for the listing on the Listing Grid.-->
<% if (listing['property_type'].match('Land')) { %>

    <%= listing['property_type'] %>
    <span class="photo-card-punct">|</span>
    <%= listing['acres']%> acres

<% } else {%>

    <%= listing['beds'] %> bd
    <span class="photo-card-punct">|</span>
    <%= listing['baths']%> ba
    <span class="photo-card-punct">|</span>
    <%= listing['square_feet'].replace(/\B(?=(\d{3})+(?!\d))/g, ",") %> sqft
<% } %>